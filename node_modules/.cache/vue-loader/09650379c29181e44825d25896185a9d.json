ange=0\r\n                 }else if(this.marginL<-488){ //img.width-window.innerWidth\r\n                    this.marginL=-488;\r\n                    this.xchange=-488\r\n                 }else{\r\n                this.marginL= this.marginL;\r\n              }\r\n              sul.style.marginLeft=this.marginL+\"px\";\r\n        }\r\n        \r\n       \r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n.myfont1{\r\n    font-size:1.5rem;\r\n    color:#000;\r\n}\r\n.myfont2{\r\n    font-size:0.9rem;\r\n    color:#8f8f94;\r\n}\r\n.myfont3{\r\n    font-size:1rem;\r\n    color:#000;\r\n}\r\n.myfont4{\r\n    font-size:0.9rem;\r\n    color:#B0B0B0;\r\n}\r\n/*.homelist{\r\n    margin:4rem auto;\r\n}*/\r\n.homestyle>ul{\r\n    height:16rem;\r\n    width:870px;\r\n    /* white-space: nowrap !important; */\r\n    /* overflow-x: auto !important; */\r\n    }\r\n    \r\n.homestyleC{\r\n    width:160px;\r\n    margin:0.3rem;\r\n    float: left;\r\n   \r\n}\r\n\r\n.homestyleImg>img{width:100%;height:100%;border-radius:5px;}\r\n.homestyleImg{\r\n    position: relative;\r\n    width: 100%;\r\n    height: 107px;\r\n}\r\n</style>\r\n\r\n"]}]}